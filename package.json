{
  "name": "trading-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "prepare": "husky",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:unit": "cypress run --component",
    "test:unit:dev": "cypress open --component",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:oxlint:check": "oxlint . -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix --cache",
    "lint:eslint:check": "eslint . --cache",
    "lint": "run-s lint:*",
    "lint:check": "run-s lint:oxlint:check lint:eslint:check",
    "format": "prettier --write --experimental-cli src/",
    "format:check": "prettier --check --experimental-cli src/",
    "generate:api": "openapi-typescript https://api.spacetraders.io/v2/documentation/json -o src/core/api/v2.d.ts"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.92.1",
    "openapi-fetch": "^0.15.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@prettier/plugin-oxc": "^0.0.5",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/cypress": "^10.1.0",
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-oxlint": "~1.29.0",
    "eslint-plugin-vue": "~10.5.1",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "openapi-typescript": "^7.10.1",
    "oxlint": "~1.29.0",
    "prettier": "3.6.2",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.0",
    "unplugin-vue-router": "^0.19.1",
    "vite": "^7.3.0",
    "vue-tsc": "^3.1.5"
  },
  "overrides": {
    "qs": "^6.14.1"
  }
}
